for (i in 1:20){
x<- rnorm(10)
ci<-c((mean(x)-qt(q, 9)*(sd(x)/sqrt(10))),
(mean(x)+qt(q, 9)*(sd(x)/sqrt(10))))
y<-y+1
if(ci[1]<=0&&ci[2]>=0){
par(new=T)
lines(c(ci[1],ci[2]), c(y,y), lty=1,lwd=3, col="steelblue")
}
else{
par(new=T)
lines(c(ci[1],ci[2]), c(y,y), lty=1,lwd=3, col="indianred")
}
}
}
