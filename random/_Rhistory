(mean(x)+qt(.975, 9)*(sd(x)/sqrt(10))))
ho<-0
y<-y+1
plot(0,0, xlim=c(-2,2), ylim=c(0,10), abline(v=0))
par(new=TRUE)
if(ci[1]<=ho&&ci[2]>=0){
lines(c(ci[1],ci[2]), c(y,y), lty=1,lwd=3, col="steelblue")
}
else{
lines(c(ci[1],ci[2]), c(y,y), lty=1,lwd=3, col="red")
}
}
